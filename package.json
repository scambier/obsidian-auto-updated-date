{
  "name": "obsidian-sample-plugin",
  "version": "0.0.1",
  "description": "This Obsidian plugin automatically adds an updated a front matter `updated` value to your notes.",
  "main": "dist/main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add assets/manifest.json assets/versions.json",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.2.0",
    "builtin-modules": "^3.2.0",
    "esbuild": "0.13.12",
    "esbuild-plugin-copy": "^1.2.1",
    "eslint": "7.12.1",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.2.1",
    "obsidian": "latest",
    "prettier": "^2.6.2",
    "prettier-eslint": "^13.0.0",
    "tslib": "2.3.1",
    "typescript": "4.4.4"
  },
  "dependencies": {
    "gray-matter": "^4.0.3"
  }
}